{
  "name": "neuroglancer",
  "description": "Visualization tool for 3-D volumetric data.",
  "license": "Apache-2.0",
  "version": "0.0.0-beta.0",
  "main": "dist/module/main_module.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/google/neuroglancer.git"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "generate-code": "node ./config/generate_code.js",
    "build-min": "webpack --config ./config/webpack.config.js --env output=min",
    "build": "webpack --config ./config/webpack.config.js --env output=dev",
    "build-module": "webpack --config ./config/webpack.config.js --env output=dev-module && rimraf ./tmp && tsc --outDir ./tmp && babel tmp --quiet --out-dir dist/module && copyfiles -u 2 dist/dev-module/*.css dist/module && copyfiles -u 2 dist/dev-module/*.bundle.* dist/module && rimraf ./tmp",
    "build-python": "webpack --config ./config/webpack.config.js --env output=python-dev",
    "build-python-min": "webpack --config ./config/webpack.config.js --env output=python-min",
    "build:watch": "webpack --config ./config/webpack.config.js --watch",
    "dev-server": "webpack serve --config ./config/webpack.config.js",
    "dev-server-python": "webpack serve --config ./config/webpack.config.js --env output=python-dev",
    "test": "karma start ./config/karma.conf.js --single-run",
    "test:watch": "karma start ./config/karma.conf.js --no-single-run",
    "benchmark": "karma start ./config/karma.benchmark.js --single-run",
    "benchmark:watch": "karma start ./config/karma.benchmark.js --no-single-run",
    "prepublishOnly": "npm run test && npm run build-module",
    "gulp": "gulp"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-import-noop": "^1.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "benchmark": "^2.1.4",
    "clang-format": "^1.4.0",
    "copy-webpack-plugin": "^6.3.1",
    "copyfiles": "^2.4.0",
    "fork-ts-checker-webpack-plugin": "^6.0.3",
    "gulp": "^4.0.2",
    "gulp-clang-format": "^1.0.27",
    "jasmine-core": "^3.6.0",
    "karma": "^5.2.3",
    "karma-benchmark": "^1.0.4",
    "karma-benchmark-reporter": "^0.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^1.3.1",
    "minimist": "^1.2.5",
    "nunjucks": "^3.2.2",
    "rimraf": "^3.0.2",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@types/codemirror": "0.0.98",
    "@types/gl-matrix": "^2.4.5",
    "@types/lodash": "^4.14.165",
    "@types/pako": "^1.0.1",
    "@types/sockjs-client": "^1.1.1",
    "@types/webpack-env": "^1.15.3",
    "codemirror": "^5.58.2",
    "css-loader": "^5.0.1",
    "gl-matrix": "^3.1.0",
    "glsl-editor": "^1.0.0",
    "glsl-strip-comments-loader": "^1.1.0",
    "html-webpack-plugin": "^5.0.0-alpha.3",
    "ikonate": "^1.1.1",
    "lodash": "^4.17.20",
    "nifti-reader-js": "^0.5.4",
    "numcodecs": "^0.1.1",
    "pako": "^2.0.1",
    "raw-loader": "^4.0.2",
    "resize-observer-polyfill": "^1.5.1",
    "sockjs-client": "^1.5.0",
    "style-loader": "^2.0.0",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^8.0.11",
    "typescript": "4.0.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.5.1"
  }
}
